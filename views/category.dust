
<MainLayout>

	{{#partial customStyles}}
		<meta name="custom-meta" content="Paradise city" />
	{{/partial}}

	{{#partial mainContent}}

		<div id="page-content" data-controller="products-category-controller">
			
			<h1>{{catName}}</h1>
			<span class="products-count">Products: {{products.length}}</span>

			<!--START PRODUCT GRID-->
			<products-grid products="{{products}}"/>
			<!--END PRODUCT GRID-->

			<br class="clear">
		</div>
	{{/partial}}

</MainLayout>
